	<xs:element name="earthquake_magnitude" type="as97:magnitude"/>
	<xs:element name="distance_to_rupture" type="as97:distance"/>
	<xs:element name="type_of_site" type="as97:sitetype"/>
	<xs:element name="type_of_fault" type="as97:faulttype"/>
	<xs:element name="uncertainties" type="as97:uncertainties"/>
