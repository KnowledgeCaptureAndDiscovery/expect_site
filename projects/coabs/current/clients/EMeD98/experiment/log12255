 (method  (is-less-or-equal
            (obj (estimate
                    (obj (spec-of required-force-ratio))
                    (for ?t)
                    ))
             (than (estimate
                      (obj (spec-of available-force-ratio))
                      (for ?t)
                      ))
               ))
