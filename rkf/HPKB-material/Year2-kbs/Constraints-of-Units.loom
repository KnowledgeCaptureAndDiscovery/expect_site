( in-package "BATTLE-SPACE" )

;;;==============================================
;;; define all the set used in this ontology

(defset Terrain-Set :is
    ( :one-of 'Unrestricted 'Restricted 'Severly-Restricted 'Open 'Closed ))

(defset Day-Or-Night-Set
    :is ( :one-of 'Day 'Night 'Night-With-Lights 'Night-Blackedout-Drive ))

(defset Ammunition-Status-Set
    :is (:one-of  'UNKNOWN  'GREEN  'RED  'BLACK 'AMBER ))

(defset Formation-Set
    :is (:one-of    'ECHELON 'BOX 'COLUMN 'DIAMOND  'LINE 'VEE 'WEDGE ))

(defset Preparation-Position-Set
    :is (:one-of 'Prepared 'Fortified 'Hasty 'Flank))

;;; Hasty is less than 24 hours of preparation

(defset Evaluation-Rates-Set
    :is (:one-of    'Best 'Better 'Good 'Poor))

(defset Characteristics-Rating-Set
    :is (:one-of 'High  'Moderate  'Low ))

(defset Likelihood-Of-Contact-Set
    :is (:one-of 'Not-Expected 'Possible 'Expected 'Possible-or-Expected ))

(defset Unit-Category-Set
    :is (:one-of 'UNKNWON 'Airborne 'Armor 'Artillery 'Self-Propelled-Artillery 'Towed-Artillery
         'Cavalry 'Ground-Maneuver 'Assault-Helicopter 'Attack-Helicopter 'Self-Propelled-Howitzer
         'Towed-Howitzer 'Infantry 'Light-Infantry  'Mechanized 'Mechanized-Infantry 'Heavy-Mechanized-Infantry
         'MLRS 'Tank 'Supply 'Foot-Infantry 'Armored-Infantry 'Mechanized-Infantry
         ))

 (defset Weapon-Type-Set
    :is (:one-of  'M19A1 'M102 'M101A1 'M114A1 'M114A2  'M109A3 'M109A4 'M109A5 'M109A6 'M198 'M270
                    '2S9 'D30 '2S1 'D20 '2S3 '2S5 '2S19 '2A36 '2A65 '2S7 '2B11 'M160 'M240
                    'BM21 'BM22 '9A52
         ))



(defconcept Geographical-Area)

;;;this concept relate to logic geographical area.

;;;;;;;; Constraints
;;;=================================================================
;;; Root Concept Constraints

(defconcept Constraints)

;;;======================================================
;;; subtype of constraints

(defconcept Time-Constraints
    :is-primitive Constraints
    :annotations ((documentation "Constraints related to time")))

(defconcept Resource-Constraints
    :is-primitive Constraints
    :annotations ((documentation "Constraints related to resource")))

(defconcept Geospatial-Constraints
    :is-primitive Constraints
    :annotations ((documentation "Constraints related to geographical factors, space,etc.")))


;;;========================================================================
;;; subtype of Time-Constraints

(defconcept Unit-Time-Related-Constraints
     :is-primitive Time-Constraints
     :roles(( unit-category     :type Unit-Category-Set)
             ( echelon :type Military-Unit)
             ( description       :type String)
             ( terrain-type    :type Terrain-Set )
             ( day-or-night    :type  Day-Or-Night-Set))
     :annotations ((documentation "Time constraints relate to units type,
     terrain type, and day-or-night conditions")))




    ;;;**************************************************************
    ;;;subtype of Unit-Time-Related-Constraints

    (defconcept Unit-Rate-Of-Movement
        :is-primitive  Unit-Time-Related-Constraints
        :roles (( maximum-speed        :type  Number
                :facets (( documentation "Maximum moving speed in Kmph")))
              ( nominal-speed     :type Number
                :facets (( documentation "Nominal moving speed in Kmph"))))
        :annotations ((documentation "Unit's maximum and nominal rate of movement.It is depended on
        the unit-category, terrain-type and day-or-night-conditions")))


;;============================================================================
 ;;;==============================================================================
 ;;; subtype of Resource-Constraints

    (defconcept Artillery-Capability
        :is-primitive Resource-Constraints
        :roles (( weapon-type   :type Weapon-Type-Set)
                ( description   :type String)
                ( minimum-range :type Integer
                    :facets ((documentation "meters")))
                ( maximum-range  :type Integer
                    :facets (( documentation "meters")))
                ( maximum-range-with-RAP :type Integer
                    :facets(( documentation " meters. RAP:Rocket-Assisted-Projectile")))
                (minimum-rate-of-fire :type Integer
                    :facets (( documentation "rounds per minute")))
                (maximum-rate-of-fire :type Integer
                    :facets (( documentation "rounds per minute"))))
        :annotations (( documentation "The artillery's capabilities can be used in determining/evaluating
                    position selections")))


	 ;;;*******************************************************
            ;;; subtype of Artillery-Capability

            (defconcept US-Artillery-Capability
                :is-primitive Artillery-Capability
                :roles (( maximum-FPF-width     :type Integer
                            :facets ((documentation "meters")))
                         (weight    :type Integer
                            :facets ((documentation "pounds")))
                        ))




 (defconcept Relative-Force-Ratios
    :is-primitive  Resource-Constraints
    :roles (( friendly-number   :type Number
                :facets (( documentation "For a specific combat mission, the number of friendly units")))
            ( enemy-number      :type  Number
                :facets ((documentation "For a specific combat mission, the number of enemy units"))))
    :annotations (( documentation "The relative ratios of friendly units number to enemy units number
    for a specific task. The division planners compare all types of combat units two level down
    when analysis the relative force ratios." )))

  ;;;;;;;;; RelativeForce-Ratio is depended on the specific COA. This works as a place holder here.


     ;;;*************************************************
     ;;;;;;subtype of Relative-Force-Ratios
     ;;;; mission type should be Action, now just put string for load test
     (defconcept Historical-Minimum-Planning-Ratios
         :is-primitive   Relative-Force-Ratios
         :roles (( mission-type      :type  String )
                    ( preparation-position          :type Preparation-Position-Set
                        :facets (( documentation " The preparation position for conduct a mission"))))
        :annotations (( documentation "The histocial minimum-planning ratios of friendly to enemy units
        required for each combat mission. \n\r\It is used to determine if the unit has the overwhelming odds
        that would give hime the flexibility to conduct the type of operation he desires. \n\r During development of the scheme of
        maneuver the planner compare these ratios to the ones he can generate in specific portions of the
        battlefield.")))

;;;========================================================================
   ;;; concept Unit-Resource-Related-Constraints

    (defconcept Unit-Resource-Related-Constraints
        :is-primitive  Resource-Constraints
        :roles (( unit-category     :type Unit-Category-Set)
            ( echelon    :type Military-Unit )
            ( description       :type String))
        :annotations ((documentation "This works as a superconcept for all unit related resource
                constraints. It define the roles unit-category, echelon, and description which are the
                common roles for all subconcpet")))

;***************************************************************
     ;;; subtype of Unit-Resource-Related-Constraints

        ;;;;------------------------------------------------------------------------
        ;;;; define concept relate to Unit-Combat-Power-Doctrinal-Constraints's role

        (defconcept Fuel-Capacity-Consumption-Constraint
        :roles ((maximum-fuel-capacity      :type  Number
                    :facets (( documentation "The maximum fuel capacity in gallons")))
                (fuel-consumption-in-idle    :type Number
                    :facets (( documentation "The consumption rate in gallons/hour when vehicle
                in idle state")))
                (fuel-consumption-moving  :type Number
                    :facets ((documentation "The consumption rate in gallons/hour when vehicle
                    in moving state"))))
        :annotations ((documentation "For each echelon and unit type, there will be its maximum capacity
        of fuel in gallons and consumption rate in gallons per hour when it is idle or moving")))



        (defconcept  Deployment-Size
            :roles ((width  :type Number)
                    (length   :type Number))
            :annotations ((documentation "There are serveral kinds of formaiton type for each echelon and
            unit type. Deployment size define the length and width sizes relate to each kind of formation. It works
            as a abstract type")))

        (defconcept Line-Size
            :is-primitive  Deployment-Size)

        (defconcept Wedge-Size
            :is-primitive Deployment-Size)

        (defconcept  Column-Size
            :is-primitive  Deployment-Size)

        (defconcept Box-Size
            :is-primitive  Deployment-Size)

        (defconcept Diamond-Size
            :is-primitive  Deployment-Size)

        (defconcept  Echelon-Size
            :is-primitive  Deployment-Size)

        (defconcept Vee-Size
            :is-primitive  Deployment-Size)

        (defconcept Deployment
            :roles ((line      :type Deployment-Size)
             (wedge     :type Deployment-Size)
             (column    :type Deployment-Size)
             (box       :type Deployment-Size)
             (diamond   :type Deployment-Size)
             (echelon   :type Deployment-Size)
             (vee       :type Deployment-Size))
        :annotations ((documentation " For each echelon and unit type, there are different kinds of deployment
            formation" )))


;;;;----------------------------------------------------
    ;;;;  Unit-Combat-Power-Doctrinal-Constraints

    (defconcept Unit-Combat-Power-Doctrinal-Constraints
        :is-primitive  Unit-Resource-Related-Constraints
        :roles (( combat-power    :type  Number )
            ( artillery-area   :type Number )
            ( unit-equivalent   :type Number)
            ( fuel-capacity-and-consumption   :type Fuel-Capacity-Consumption-Constraint)
            ( unit-deployment-size   :type Deployment)
         )
        :annotations (( documentation "For each echelon and unit type, there are a bunch of information such as
        combat-power, artillery-area, unit-equipment , fuel-capacity-and consumption,and deployment-size
         relate to it.")))


    (defconcept Unit-Readiness-Strength-Ammunition-Status
        :is-primitive Unit-Resource-Related-Constraints
        :roles (( readiness     :type Number )
                ( strength      :type Number )
                ( ammunition-status :type Ammunition-Status-Set)
            )
   )

     ;;;; A place holder which is relate to a specific task action.
     ;;;; Provide informaiton about the unit's status before or after performing an action, the unit's status.



    ;;;; from document Task Move tactically, this is define a doctinal frontage size.

    (defconcept Unit-Firing-Power
        :is-primitive Unit-Resource-Related-Constraints
        :roles (( range     :type Number)
                 ( rate      :type Number))
        :annotations ((documentation "The fire covered range and rate of fire which
        may be delivered by a unit, or weapon system.")))




    (defconcept Unit-Enviromental-Conditions
        :is-primitive Unit-Resource-Related-Constraints
        :roles (( sound        :type String)
             ( illumination :type   String)
             ( heat         :type   String)
             ( visibility    :type   String))
        :annotations ((documentation "This is related to assembly area. If an assembly area is choosen
        and unit moving to it requires minimum noise, heat, radio, traffic, and visible light." )))

    (defconcept Artillery-Unit-Firing-Position
        :is-primitive Unit-Resource-Related-Constraints
        :roles (( artillery-type :type String )
            ( minimum-position   :type Number
                :facets ((documentation "square kilometer")))
            ( desired-position   :type Number
                 :facets ((documentation "square kilometer"))))
        :annotations (( documentation " The minimum and desired position sizes in  for
        each kinds of artillery units"))
        )



;;;==========================================================================
;;;=========================================================================
;;; subtype of Geospatial-Constraints

(defconcept Unit-Doctrinal-Frontage-Size
        :is-primitive  Geospatial-Constraints
        :roles((unit-category   :type Military-Unit)
               (number-of-units :type Integer)
               (frontage-size   :type Integer
                    :facets ((documentation "The unit's doctrinal frontage size in km"))))
        :annotations ((documentation "A units calculates the width of its frontage by adding
        the width of all its subordinate units with the total width between each of its subordinate units."))
    )

(defconcept Area-Of-Operation
      :is (:and Geospatial-Constraints
            Geographical-Area)
    :roles (( offense-or-defense    :type Integer)
            ( axis-of-advance       :type Geographical-Region)
            ( zone-of-action        :type Geographical-Region)
            ( sector                :type Geographical-Region)
            ( battle-position       :type Geographical-Region))
    :annotations ((documentation "An operational area defined by the joint force commander for
    land and naval forces. Areas of operation do not typically encompass the entire operational
    area of the joint force commander, but should be large enough for component commanders to
    accomplish their missions and protect their forces. (Army) - A geographical area, usually
    defined by lateral, forward, and rear boundaries assigned to a commander, by a higher
    commander, in which he has responsibility and the authority to conduct military operations."
    )))

 (defconcept Mobility-Corridor-Doctrinal-Width
     :is (:and Geospatial-Constraints
            Geographical-Area)
     :roles (( unit-category    :type Military-Unit)
             ( width        :type Number
                :facets (( documentation "Kilometers"))))
    )

 (defconcept Mobility-Corridor
     :is (:and Geospatial-Constraints
            Geographical-Area)
    :roles (( echelon-level-of-concern  :type Military-Unit)
            ( concerned-mobility-corridor          :type Mobility-Corridor-Doctrinal-Width
                :facets ((documentation "A unit looks down two echelons when it is looking
                            for mobility-corridors"))))
     :annotations ((documentation "Areas where a force will be canalized due to terrain constructions.
     mobility corridor is relatively free of obstacles and allows military forces to
    capitalize on the principles of mass and speed. When identifying a mobility corridor a unit
    is looking for relatively open areas through which it can move when deployed. Mobility corridors
    characteristically follow the direction of roads and trails and, generally traverse unrestricted
    terrain.")))





    (defconcept Approach-Avenue
     :is (:and Geospatial-Constraints
                Geographical-Area
               (exactly  1  type-of-echelon)
               (at-least 1  corridor-of-moving)
               (exactly  1  max-distance-between-mobility-corridor))
     :annotations ((documentation "Avenues of Approach are determined by combining two or more
     mobility corridors, provide the corridors are close enough together to provide for mutual support.
     A unit will look down one echelon when it is looking for Avenues of Approach.")))

     ;;;;;
     ;;;;Note: we need add a LOOM axiom to calculate an Avenue-Of-Approach width
     ;;;;based on number of mobility corridors (MCs), width
     ;;;;of mobility corridors and distances between MCs.




(defconcept Maneuver-Room
   :is  (:and Geospatial-Constraints
               Geographical-Area
               (at-least 1 avenue-of-approach)
               (exactly  1 open-or-closed-terrain))
   :annotations ((documentation "Maneuver Room is defined as the total number of avenue of
                 approaches(AA) and the maneuver space(width) of each of the AAs, and whether the
                 terrain is open ot close.")))



   ;;;;
   ;;;;Note: we need add a LOOM axiom to calculate a maneuver
   ;;;;romm size based on number of avenue of approaches (AAs), widths of AAs and open or
   ;;;;close terrain.



 ;;;;;;;;;;;;; relations  ;;;;;;;;;;;;;;;;;;;;;;
(defrelation    type-of-echelon
    :domain     Approach-Avenue
    :range      Military-Unit)

(defrelation      corridor-of-moving
    :domain      Approach-Avenue
    :range       Mobility-Corridor)

(defrelation    max-distance-between-mobility-corridor
    :domain     Approach-Avenue
    :range      Number
    :annotations ((documentation "Kilometers")))

(defrelation      avenue-of-approach
        :domain   Maneuver-Room
        :range    Approach-Avenue)

(defrelation    open-or-closed-terrain
        :domain     Maneuver-Room
        :range      Terrain-Set)


;;;;;;;;;;;;;;;;;;; subtype of Geospatial-Constraints;;;;;;;;;;;;;;;;;;;;;;;;

(defconcept Engagement-Area
    :is (:and Geospatial-Constraints
            Geographical-Area)
    :roles (( range  :type Integer)
            (ideal-planning-range   :type Integer))
    :annotations ((documentation "engagement area (EA) is an area along an enemy avenue of
    approach where the commander intends to contain and destroy an enemy force with the massed
    fires of all available weapons. The size and shape of the engagement area is determined by
    the relatively unobstructed intervisibility from the weapon systems in their firing positions
    and the maximum range of those weapons. Sectors of fire are usually assigned to subordinates
    to prevent fratricide.")))

;;;comment Engagement-Criteria is a place holder for possible roles relate to this class
(defconcept Engagement-Criteria
   :is-primitive  Geospatial-Constraints
   :annotations ((documentation "Those circumstances that allow engagement of an enemy force
   without a specific command to do so. Examples are a point or line on the ground that an
   enemy crosses or an event or action that an enemy does."))
   )

;;;comment Engagement-Priority is a place holder for possible roles relate to this class
(defconcept Engagement-Priority
   :is-primitive  Geospatial-Constraints
   :annotations ((documentation "The sequence for attack of targets in order of importance
   assigned to specific weapon systems to facilitate the destruction of threat vehicles
   expected in an armored formation. Engagement priorities are assigned based on the type
   or level of threat at different ranges to best match organic weapon system capabilities
   against threat vulnerabilities."))
   )

;;;;======================
;;; Assembly-Area (AA)

(defconcept Assembly-Area
   :is (:and Geospatial-Constraints
            Geographical-Area
              (exactly 1 accessibility)
              (exactly 1 intervisibility)
              (exactly 1 trafficability)
              (at-least 1 criteria-of-evaluation)
              (at-least 1 fields-of-fire)
              (at-least 1 size-enumeration))
   :annotations ((documentation "An area in which a command is assembled preparatory
   to further action.")))

(defconcept Evaluation-Ranking
    :is-primitive Geospatial-Constraints
    :roles ((rate           :type Evaluation-Rates-Set)
            (point-weight    :type Integer)))


(defconcept Evaluation-Criteria
     :is-primitive Geospatial-Constraints
     :roles ((woods  :type Evaluation-Ranking)
             (severly-restricted-terrain-inside-AA  :type Evaluation-Ranking)
             (roads-and-trails  :type Evaluation-Ranking)
             (terrain-shielding :type Evaluation-Ranking)
             (fields-of-fire    :type Evaluation-Ranking)
             (grouping          :type Evaluation-Ranking))
     :annotations ((documentation "The evaluation criteria are derived from a careful examination of each
     particular case and collective judgement in rank ording and weighting. It evaluates the selection of a Assembly Area in
     quantitatively based on a point system.")))




;;;;;;==========================
;;;; relation relate to AA

(defrelation accessibility
    "Assembly Area (AA) needs to be accessible for all units that will
    entry AA."
    :domain Assembly-Area
    :range String
   )


(defrelation  intervisibility
    "Desire to place an Assembly Area(AA) with high ground dividing AA
    and enemy."
    :domain Assembly-Area
    :range String
)

(defrelation  trafficability
     "Terrain within Assembly Area must be trafficable by all."
    :domain Assembly-Area
    :range String
)

(defrelation  criteria-Of-Evaluation
    "in quantitatively evaluated the selection of a Assembly-Area based
                on a point system."
    :domain  Assembly-Area
    :range  Evaluation-Criteria
)


 (defrelation size-enumeration
    :domain Assembly-Area
    :range String)


;;;;;;;;================================
;;; concept Boundary

(defconcept  Boundary
    :is-primitive  Geospatial-Constraints)

;;;;;;; subtype of Boundary;;;;;;;

(defconcept Forward-Line-Of-Troops
    :is-primitive Boundary
    :annotations ((documentation "A line which indicates the most forward positions of friendly forces
    in any kind of military operation at a specific time.")))



(defconcept Line-Of-Departure
    :is-primitive Boundary
    :annotations (( documentation "A line designated to coordinate the departure of attack elements.")))

(defconcept Phase-Line
    :is-primitive
        ( :and Boundary
           (at-least 1 coordination-of-line)
           (at-least 1 limit-of-advance)
           (at-least 1 passage-lane))
    :annotations ((documentation "A line used to control and coordination.")))



(defconcept Coordination-fire-Line
    :is-primitive Boundary
    :annotations (( documentation "A Line beyond which conventional surface fires support.")))

(defconcept Limit-Of-Advance
    :is-primitive Boundary
    :annotations (( documentation "An easily recognized terrain feature beyond
    which attacking elements will not advance.")))

(defconcept Passage-Lane
	:is-primitive (:and Boundary
	(exactly 1 size-of-force))
)
(defrelation size-of-force
	:domain Boundary
	:range Number)



;;;;;; relations relate to Phase-Line  ;;;;;;
(defrelation coordination-of-line
    :domain Phase-Line
    :range Coordination-fire-Line)

(defrelation    limit-of-advance
    :domain Phase-Line
    :range  Limit-Of-Advance)

(defrelation passage-lane
    :domain  Phase-Line
   :range  Passage-Lane)



