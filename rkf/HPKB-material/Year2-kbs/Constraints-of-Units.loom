;;;;concept Unit-Immediate-Area

(defconcept Unit-Immediate-Area
    :is-primitive (:and Geospatial-Constraints
              (at-least 1 of-battle))
    :annotations ((documentation "A unit's immediate area encompasses an area extending out
    from its current defensive positions to the range of an enemy's direct fire weapons. It is
    defined by \"position\" and \"Enemy weapon system\".")))

(defrelation of-battle
    :domain Unit-Immediate-Area
    :range Battle)




