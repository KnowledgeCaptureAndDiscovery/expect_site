 ;;;;;;;;;; concept Formation-Selection-Constraint

 (defconcept Formation-Selection-Constraint
   :is (:and Geospatial-Constraints
            Geographical-Area
          (exactly 1 formation-type)
          (exactly 1 minimum-subunit-required)
          (at-least 1 requirement-of-frontage)
          (exactly 1 room-of-maneuver)
          (exactly 1 characteristic-of-formation))
   :annotations ((documentation "How a unit selects a particular formation is determined by
   applying four criteria: Formation size, Maneuver Room, Frontage Requirement, and Enemy Situation.
   All these information will help a critiquer to check the feasibility of the unit formations
   chosen in a COA.")))




(defconcept Formation-Characteristic
    :is (:and Geospatial-Constraints
            Geographical-Area)
    :roles (( formation      :type Formation-Set)
            ( likelihood-of-contact-enemy      :type Likelihood-Of-Contact-Set
                :facets(( documentation "Using a base figure of five kilometers a unit can
                    determine the likelihood of contact with enemy forces" )))
            ( combat-power-forward      :type  Characteristics-Rating-Set
                :facets (( documentation "Provides maximum firepower forward")))
            ( command-and-control       :type Characteristics-Rating-Set
                :facets (( documentation "Control of subordinate units is easier")))
            ( flexibility               :type  Characteristics-Rating-Set
                :facets (( documentation "Provides for quick transition to another foramtion or
                            movement against a detected threat" )))
            ( flank-security             :type  Characteristics-Rating-Set
                :facets (( documentation "Provides all around security of the force")))
            ( speed                      :type  Characteristics-Rating-Set
                :facets (( documentation "Facilitates speed of movement")))
            ( maneuver-space             :type  Characteristics-Rating-Set
                :facets (( documentation "Allows sufficient space for a higher headquarters to maneuver
                                        subordinates in and/or out of various formations" )))
          )
     :annotations (( documentation " A unit selects a formation by comparing what is known or unknown
                    about the enemy with the advantages and disadvantages of ezch possible formation." ))
  )



(defconcept Frontage-Requirement
    :is (:and Geospatial-Constraints
            Geographical-Area)
    :roles (( formation     :type Formation-Set)
            ( subunit-numbers   :type Integer)
            ( units-frontage-requirement  :type Integer))
    :annotations ((documentation "Frontage Requirement is defined as the total width of a unit's
    frontage. A unit calculated the width of its frontage by adding the width of all of its
    subordinate unit's with the total width between each of its subordinate units.")))

;;;;;;;;;;;;relations relate to Formation-Selection-Constraint;;;;;;;;;;;;;;;;;;

 (defrelation  formation-type
    :domain Constraints
    :range Formation-Set)

(defrelation minimum-subunit-required
   :domain  Formation-Selection-Constraint
   :range Integer)

  (defrelation   requirement-of-frontage
    :domain     Formation-Selection-Constraint
    :range      Frontage-Requirement)

 (defrelation   room-of-maneuver
    :domain     Formation-Selection-Constraint
    :range      Maneuver-Room)

 (defrelation   characteristic-of-formation
    :domain     Formation-Selection-Constraint
    :range      Formation-Characteristic )




