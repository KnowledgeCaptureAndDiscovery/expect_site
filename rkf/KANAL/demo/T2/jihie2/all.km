(Neutralize-Unit has (superclasses (Engagement-Military-Task)))

(every Neutralize-Unit has
       (agent ((a Military-Unit)))
       (object ((a Military-Unit)))

       (del-list ((:triple (the object of Self)
                           combat-strength
                           (the combat-strength of (the object of Self)))))

       (add-list ((:triple (the object of Self)
                           combat-strength
                           (a Combat-Strength-Value with
                              (value ((:pair *neutralized Military-Unit)))))))
)

