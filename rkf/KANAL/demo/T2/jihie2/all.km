;;  $Id: Engagement-Military-Task.km,v 1.3 2002/08/16 13:18:13 kbarker Exp $
;;

(Engagement-Military-Task has
   (superclasses (Military-Task))
)

(every Engagement-Military-Task has
   (object ((a Military-Unit)))     ;; known to be the enemy for 
                                    ;; Engagement-Military-Tasks

   ;(required-force-ratio 
   ;   ((a Required-Force-Ratio-Value with
   ;       (value ((:pair (a Number) NIL))))))

   (available-force-ratio 
      ((a Available-Force-Ratio-Value with
          (value ((:pair ((the sum of
                               (the bag of
                                    (the1 of 
                                          (the value of
                                               (the relative-combat-power of
                                                    (the agent of Self))))))
                          /
                          (the sum of
                               (the bag of
                                    (the1 of 
                                          (the value of
                                               (the relative-combat-power of
                                                    (the object of Self)))))))
                         NIL)))
   )))

#| jihie 
made this as a soft pcs because user may still perform the task without enough
ratio (and KANAL can continue simulation)

   (pcs-list
      ((if ((the1 of (the required-force-ratio of Self)) /= 0)
        then
           (:triple (the available-force-ratio of Self)
                    >=
                    (the required-force-ratio of Self)))
   ))
|#

   (soft-pcs-list
      ((if ((the1 of (the value of (the required-force-ratio of Self))) /= 0)
        then
           (:triple (the1 of (the value of (the available-force-ratio of Self)))
                    >=
                    (the1 of (the value of (the required-force-ratio of Self)))))
   ))

   (del-list 
      	((forall (the object of Self)
           (:triple 
               It 
               remaining-strength
               (the remaining-strength of It)))
   ))

   (add-list 
      ((forall (the object of Self)
           (:triple 
               It 
               remaining-strength
               (a Remaining-Strength-Value with
                  (value ((:pair ((the1 of (the value of
				   (the object-attrition-rate of Self)))
                                    * (the1 of (the value of
					(the remaining-strength of It))))
                                 NIL))))))
   ))
)


