  (test-case ('( 
                 (*Fact has 
			(instance-of (Information)))
                 (*Bob has 
			(instance-of (Tangible-Entity)))
                 (*Alice has 
			(instance-of (Tangible-Entity)))

		 ;; Bob expressed to Alice the Fact
                 (*Express has
                        (instance-of (Express))
                        (agent (*Bob))
                        (recipient (*Alice))
                        (object (*Fact))
		 )

                 (do-and-next *Express)
		 
		 (the text-gen of (thelast Express))

		 ;;Did Bob know the Fact?
		 ((the experiencer of (thelast Be-Known)) = *Bob)
		 ((the object of (thelast Be-Known)) = *Fact)

		 ;;Can Alice interpret the Fact?
		 (do-and-next (the enables of (thelast Express)))

		 ((the agent of (thelast Interpret)) = *Alice)
