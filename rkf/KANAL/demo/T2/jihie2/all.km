;; 

(Defeat-Unit has 
        (superclasses (Engagement-Military-Task))
        (description ("An action in which one military unit defeats another")))

(every Defeat-Unit has
       (agent ((a Military-Unit)))
       (object ((a Military-Unit)))

       (del-list ((:triple (the object of Self)
                           combat-strength
                           (the combat-strength of (the object of Self)))))

       (add-list ((:triple (the object of Self)
                           combat-strength
                           (a Combat-Strength-Value with
                              (value ((:pair *defeated Military-Unit)))))))
)

;; See also Neutralize-Unit, Destroy-Unit, Annihilate-Unit and
;; Reconstitute-Unit; these are all Actions that set the combat-strength
;; property of a Military-Unit.

;; Ideally, Actions that set scalar values would be arranged taxonomically
;; to match the scale (Make-Very-Bad would be a subclass of Make-Bad, e.g.).
;; That would require some kind of subsumption mechanism for scale values.

;; 
;;  $Id: Delay.km,v 1.4 2002/08/14 19:21:42 tecuci Exp $
;;

(Delay has
       (superclasses (Engagement-Military-Task))
       (description ("a task to slow enemy advance from reaching a friendly objective")))

(every Delay has
       (agent ((a Military-Unit)))
       (object 
	((a Military-Unit with
	    (agent-of 
	     ((a Advance with
		 (toward ((must-be-a Entity with
				     (allegiance ((the allegiance of
						       (the agent of Self))))))))))
	    )))
       
       (del-list ((forall (the Advance agent-of of (the object of Self))
			  (:triple It rate (the rate of It))
			  )))
       
       (add-list ((forall (the Advance agent-of of (the object of Self))
			  (:triple It rate (a Rate-Value with
					      (value ((:pair *slow Advance)))))
			  )))
       
       (text-active-verbseq-sg ("delays"))
       (text-active-verbseq-pl ("delay"))
       
       (text-argslots ((:seq)))
       )
 

