                 (*Money has (instance-of (Object)))
                 (*Fred has
                        (instance-of (Living-Entity)) 
                        (possesses   (*Money)))
                 (*Steven has (instance-of (Tangible-Entity)))

		 ;; *Fred Supplied *Money to *Steven
                 (*Supply has
                        (instance-of (Supply))
                        (agent       (*Fred))
                        (recipient   (*Steven))
                        (object      (*Money)))
                 (do-and-next *Supply)
		 (the text-gen of (thelast Supply))
		

		 ;; Test 1: Supply enables an Obtain
