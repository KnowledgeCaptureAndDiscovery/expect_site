;;
;;  $Id: Military-Task.km,v 1.3 2002/08/15 21:25:47 kbarker Exp $
;;

(Military-Task has
   (superclasses (Action))
)

;; Here's how the preconditions work:
;; IF this Military-Task is also a Move
;;       AND
;;    the path of this Move/Military-Task has a trafficability defined
;;       AND
;;    at least one of the trafficability values applies to this kind of object
;; THEN
;;    the trafficability of the path for this kind of object must not be *SR
;;       AND
;;    it ideally should be *UR (soft precondition)

(every Military-Task has
   (pcs-list ((if ((Self isa Move)
                   and
                   (has-value (the trafficability of (the path of Self)))
                   and
                   (has-value (oneof (the value of (the trafficability of 
                                                    (the path of Self)))
                                     where ((the object of Self)
                                            isa
                                            (the2 of It))))
                  )
               then 
                  (forall (the value of (the trafficability of
                                             (the path of Self)))
                          where ((the object of Self) isa (the2 of It))
                          (:triple (the1 of It) /= *SR))
   )))

   (soft-pcs-list
             ((if ((Self isa Move)
                   and
                   (has-value (the trafficability of (the path of Self)))
                   and
                   (has-value (oneof (the value of (the trafficability of 
                                                    (the path of Self)))
                                     where ((the object of Self)
                                            isa
                                            (the2 of It))))
                  )
               then 
                  (forall (the value of (the trafficability of
                                             (the path of Self)))
                          where ((the object of Self) isa (the2 of It))
                          (:triple (the1 of It) = *UR))
   )))

;; jihie
;; Move-Military-Unit (a Non-Engagement-Military-Task) uses 'object'
;; instead of 'agent' for the unit assigned so I moved
;; add/del of object-attrition-rate from Engagement-Military-Task
;; to here 
 
   (del-list 
      ((if (Self isa Move)
        then (forall (the object of Self)
              (:triple 
               It 
               remaining-strength
               (the remaining-strength of It)))
        else (forall (the agent of Self)
              (:triple 
               It 
               remaining-strength
               (the remaining-strength of It))))
    ))
       
   (add-list 
      ((if (Self isa Move)
        then (forall (the object of Self)
              (:triple 
               It 
               remaining-strength
               (a Remaining-Strength-Value with
                  (value ((:pair ((the1 of (the value of
				   (the object-attrition-rate of Self)))
                                    * (the1 of (the value of
					(the remaining-strength of It))))
                                 NIL))))))
        else (forall (the agent of Self)
              (:triple 
               It 
               remaining-strength
               (a Remaining-Strength-Value with
                  (value ((:pair ((the1 of (the value of
				   (the agent-attrition-rate of Self)))
                                    * (the1 of (the value of
					(the remaining-strength of It))))
                                 NIL)))))))
       
   ))
)


;;
;; $Id: Military-Unit.km,v 1.7 2002/08/16 21:19:41 kbarker Exp $
;;

(Military-Unit has (superclasses (Aggregate Tangible-Entity)))

(every Military-Unit has
   (allegiance ((the allegiance of (the element-of of Self))))

   #|(default-combat-power 
      ((a Default-Combat-Power-Value with
          (value ((:pair (the max of
                              (the bag of
                                   (the1 of
                                         (the default-combat-power of
                                              (the Military-Equipment possesses
                                                   of Self)))))
                         NIL)))
   )))|#

   ;(remaining-strength ((a Remaining-Strength-Value with
   ;                        (value ((:pair 1 NIL)))          ;; to start
   ;)))

;; BIG assumption here: that the values of default-combat-power and
;; remaining-strength will never get more than one :pair in them

   (relative-combat-power 
      ((a Relative-Combat-Power-Value with
          (value ((:pair ((the1 of 
                                (the value of 
                                     (the remaining-strength of Self)))
                          *
                          (the1 of
                                (the value of
                                     (the default-combat-power of Self))))
                         NIL)))
   )))
)


